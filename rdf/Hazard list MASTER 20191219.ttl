hazard:
  a owl:Ontology ;
  rdfs:comment """Nesting achieved by this SPARQL query: 

INSERT { 	?collc a skos:Collection ; 
								skos:prefLabel ?hc ;
								skos:member ?this . 
							?collt a skos:Collection ; 
								skos:prefLabel ?ht ;
								skos:member ?collc . }
WHERE {
	?this hazard:hAZARDCLUSTER ?hc .
	?this hazard:hAZARDTYPE ?ht . 
	BIND ( IRI( CONCAT( \"http://reliefweb.int/references/hazard/\" , REPLACE( ?hc , \"[ ()/]\" , \"_\") ) ) AS ?collc )
	BIND ( IRI( CONCAT( \"http://reliefweb.int/references/hazard/\" , REPLACE( ?ht , \"[ ()/]\" , \"_\") ) ) AS ?collt )
} 
""" ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
.
